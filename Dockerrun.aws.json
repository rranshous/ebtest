{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "docker-socket",
      "host": {
        "sourcePath": "/var/run/docker.sock"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "logspout",
      "image": "rranshous/logspout-loggly",
      "essential": false,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "docker-socket",
          "containerPath": "/tmp/docker.sock",
          "readOnly": false
        }
      ]
    },
    {
      "name": "ebtest-app",
      "image": "rranshous/ebtest",
      "essential": true,
      "memory": 128
    },
    {
      "name": "nginx-proxy",
      "image": "rranshous/ebtestnginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "ebtest-app"
      ]
    }
  ]
}
